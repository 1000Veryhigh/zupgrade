---
- nodejs-extensions:
  - ".js"
  - ''
  template-files:
  - ".html"
  - ".mustache"
  - ".hbs"
  - ".hdbs"
  - ".ejs"
  - ".dust"
  - ".json"
  - ".tl"
  - ".tpl"
  - ".tmpl"
  - ".pug"
  - ".ect"
  - ".sh"
  - ".yml"
  - ".toml"
  - ".ini"
  - ".xml"
  ignore-filenames:
  - ".DS_Store"
  - jquery.min.js
  - bootstrap.js
  - bootstrap-tour.js
  - raphael-min.js
  - tinymce.min.js
  - tinymce.js
  - codemirror-compressed.js
  - codemirror.js
  ignore-paths:
  - __MACOSX
  - node_modules
  ignore-extensions:
